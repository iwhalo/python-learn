    color: #666;
    background: #fff;
    border: 1px solid #ddd;
    border-radius: 3px;
    cursor: pointer;
    opacity: 0;
    transition: opacity 0.2s;
}

pre:hover::after {
    opacity: 1;
}

pre:active::after {
    background: #f0f0f0;
}

code {
    font-family: Consolas, Monaco, 'Courier New', monospace;
    font-size: 0.9em;
    background-color: #f8f8f8;
